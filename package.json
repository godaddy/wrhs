{
  "name": "wrhs",
  "description": "Robust CLI for the next generation of Object ledger and CDN",
  "version": "1.3.1",
  "author": "GoDaddy.com Operating Company LLC",
  "bin": {
    "wrhs": "./bin/run"
  },
  "bugs": "https://github.com/godaddy/wrhs/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^6.0.12",
    "warehouse.ai-api-client": "^7.1.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.25.1",
    "@oclif/test": "^3.1.14",
    "auto-changelog": "^2.4.0",
    "c8": "^9.1.0",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.0.2",
    "eslint-plugin-mocha": "^10.2.0",
    "mocha": "^10.2.0",
    "prettier": "^3.2.5",
    "sinon": "^17.0.1"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=7.6.0"
  },
  "files": [
    "/bin",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/godaddy/wrhs",
  "keywords": [
    "warehouse",
    "ledger",
    "asset",
    "object",
    "cdn"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "wrhs",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/godaddy/wrhs",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "c8 mocha \"test/**/*.test.js\"",
    "version": "oclif-dev readme && auto-changelog -p && git add README.md CHANGELOG.md",
    "lint": "npm run prettier && npm run eslint",
    "eslint": "eslint './{src, test}/**/*.js'",
    "prettier": "prettier --write src test"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent"
  }
}
