{
  "name": "wrhs",
  "version": "0.0.0",
  "description": "Flawless CLI management of build pipelines & asset deployments for your front-end apps powered by Warehouse.ai.",
  "main": "src/index.js",
  "bin": {
    "wrhs": "./bin/run"
  },
  "scripts": {
    "lint": "eslint-godaddy src/*.js src/**/*.js test/*.js test/**/*.js",
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "prepack": "oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "pretest": "npm run lint",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "homepage": "https://github.com/warehouseai/wrhs#readme",
  "author": "GoDaddy.com Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/warehouseai/wrhs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/warehouseai/wrhs.git"
  },
  "keywords": [
    "warehouse.ai",
    "wrhs",
    "warehouse",
    "webpack",
    "browserify",
    "s3",
    "cloud",
    "npm",
    "build",
    "serverless",
    "babel",
    "oclif"
  ],
  "dependencies": {
    "@oclif/command": "^1.4.34",
    "@oclif/config": "^1.6.33",
    "@oclif/plugin-help": "^2.0.5",
    "chalk": "^2.4.1",
    "debug": "^3.1.0",
    "qs": "^6.5.2",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "tty-table": "^2.6.8",
    "warehouse.ai-api-client": "^3.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.15.4",
    "@oclif/test": "^1.1.0",
    "assume": "^2.1.0",
    "chai": "^4.1.2",
    "eslint": "^5.1.0",
    "eslint-config-godaddy": "^2.1.0",
    "eslint-config-oclif": "^2.0.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mocha": "^5.1.0",
    "mocha": "^5.2.0",
    "nock": "^9.4.2",
    "nyc": "^12.0.2",
    "sinon": "^6.1.3"
  },
  "oclif": {
    "commands": "./src/commands",
    "topics": {
      "get": {
        "description": "get information about things in warehouse"
      }
    },
    "bin": "wrhs",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
