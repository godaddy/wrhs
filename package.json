{
  "name": "wrhs",
  "version": "0.4.0",
  "description": "Flawless CLI management of build pipelines & asset deployments for your front-end apps powered by Warehouse.ai.",
  "main": "src/index.js",
  "bin": {
    "wrhs": "./bin/run"
  },
  "scripts": {
    "lint": "eslint-godaddy src/*.js src/**/*.js test/*.js test/**/*.js",
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "prepack": "oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "pretest": "npm run lint",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "engines": {
    "node": ">= 8.0.0"
  },
  "homepage": "https://github.com/warehouseai/wrhs#readme",
  "author": "GoDaddy.com Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/warehouseai/wrhs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/warehouseai/wrhs.git"
  },
  "keywords": [
    "warehouse.ai",
    "wrhs",
    "warehouse",
    "webpack",
    "browserify",
    "s3",
    "cloud",
    "npm",
    "build",
    "serverless",
    "babel",
    "oclif"
  ],
  "dependencies": {
    "@oclif/command": "^1.5.13",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-help": "^2.1.6",
    "chalk": "^2.4.2",
    "cli-columns": "^3.1.2",
    "debug": "^4.1.1",
    "terminal-link": "^1.3.0",
    "tinythen": "^1.0.1",
    "warehouse.ai-api-client": "^4.4.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "assume": "^2.2.0",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-config-godaddy": "^3.0.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^5.3.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nyc": "^13.3.0",
    "sinon": "^7.3.2"
  },
  "oclif": {
    "commands": "./src/commands",
    "topics": {
      "get": {
        "description": "get information about things in warehouse"
      }
    },
    "repositoryPrefix": "<%- repo %>/blob/<%- version %>/<%- commandPath %>",
    "bin": "wrhs",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
